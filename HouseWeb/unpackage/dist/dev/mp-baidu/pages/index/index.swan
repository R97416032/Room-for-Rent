<view class="content"><view class="titletal"><text class="title">登录页面</text></view><view class="inputView"><image class="nameImage" src="../static/name.png"></image><label class="loginLab">账号</label><input class="inputText" type="text" placeholder="请输入账号" value="{{uname}}" data-event-opts="{{[['input',[['__set_model',['','uname','$event',[]]]]]]}}" bindinput="__e"/></view><view class="line"></view><view class="inputView"><image class="keyImage" src="../static/key.png"></image><label class="loginLab">密码</label><input class="inputText" password="true" type="text" placeholder="请输入密码" value="{{passw}}" data-event-opts="{{[['input',[['__set_model',['','passw','$event',[]]]]]]}}" bindinput="__e"/></view><view class="loginBtnView"><button data-event-opts="{{[['tap',[['goto']]]]}}" class="loginBtn" bindtap="__e">登录</button></view><view class="registerBtnView"><navigator url="register"><button class="registerBtn" type="default">注册</button></navigator></view></view>