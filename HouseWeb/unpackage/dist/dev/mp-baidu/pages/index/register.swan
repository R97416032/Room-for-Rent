<view class="content"><view class="titletal"><text class="title">注册页面</text></view><view class="inputView"><image class="nameImage" src="../static/name.png"></image><label class="loginLab">账号</label><input class="inputText" type="text" placeholder="请输入要注册的账号" value="{{uname}}" data-event-opts="{{[['input',[['__set_model',['','uname','$event',[]]]]]]}}" bindinput="__e"/></view><view class="line"></view><view class="inputView"><image class="keyImage" src="../static/key.png"></image><label class="loginLab">密码</label><input class="inputText" password="true" type="text" placeholder="请输入密码" value="{{passw}}" data-event-opts="{{[['input',[['__set_model',['','passw','$event',[]]]]]]}}" bindinput="__e"/></view><view class="line"></view><view class="inputView"><image class="keyImage" src="../static/key.png"></image><label class="loginLab">密码</label><input class="inputText" type="password" placeholder="请再次确认密码" value="{{passt}}" data-event-opts="{{[['input',[['__set_model',['','passt','$event',[]]]]]]}}" bindinput="__e"/></view><view class="line"></view><view class="inputView"><image class="keyImage" src="../static/key.png"></image><label class="loginLab">姓名</label><input class="inputText" type="text" placeholder="请输入您的姓名" value="{{namew}}" data-event-opts="{{[['input',[['__set_model',['','namew','$event',[]]]]]]}}" bindinput="__e"/></view><view class="line"></view><view class="inputView"><image class="keyImage" src="../static/key.png"></image><label class="loginLab">手机</label><input class="inputText" type="text" placeholder="请输入您的手机号" value="{{numw}}" data-event-opts="{{[['input',[['__set_model',['','numw','$event',[]]]]]]}}" bindinput="__e"/></view><view class="registerBtnView"><button data-event-opts="{{[['tap',[['goto']]]]}}" class="registerBtn" bindtap="__e">确认注册</button></view></view>